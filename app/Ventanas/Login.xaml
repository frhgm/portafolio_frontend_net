<Window
    x:Class="app.Ventanas.Login"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Login"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid
        Width="800"
        Height="450"
        Visibility="Visible">
        <TextBox
            Name="TxtRut"
            Height="30"
            Margin="343,143,337,265"
            TextWrapping="Wrap" />
        <PasswordBox
            x:Name="TxtPass"
            Width="120"
            Height="30"
            Margin="343,205,337,140" />
        <Label Margin="343,109,357,296" Content="Ingrese su RUT" />
        <Label Margin="343,200,307,185" Content="Ingrese su contraseña" />
        <Button
            Name="Btn_Login"
            Margin="0,316,0,0"
            Click="BtnLogin_OnClickAsync"
            Content="Iniciar sesion" />


    </Grid>
</Window>
