<Window x:Class="app.Ventanas.CrearUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="CrearUsuario" Width="800" Height="900">
    <Grid>
        <TextBox Name="Txt_Rut"></TextBox>
        <TextBox Name="Txt_Nombre"></TextBox>
        <TextBox Name="Txt_ApellidoPaterno"></TextBox>
        <TextBox Name="Txt_ApellidoMaterno"></TextBox>
        <TextBox Name="Txt_Telefono"></TextBox>
        <TextBox Name="Txt_Correo"></TextBox>
        <TextBox Name="Txt_FechaNacimiento"></TextBox>
        <TextBox Name="Txt_Rol"></TextBox>
        <TextBox Name="Txt_Clave"></TextBox>

    </Grid>
</Window>
